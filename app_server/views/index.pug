extends layout
block content

  div
    img.col-md-12.img-fluid(src= imageLocation alt='generalHealth')
  div
    table.col-md-12.table.table-striped.table-hover.table-bordered
      thead.thead-dark
      each header in exerciseHeaders
        tr
          th= header.nameCol
          th= header.warmUpCol
          th= header.setsCol
          th= header.repsCol
          th= header.repMaxCol
          th= header.restCol
          th= header.demoCol
      tbody
      each exercise in exercises
        tr
          td= exercise.name
          td= exercise.warmUpSets
          td= exercise.sets
          td= exercise.reps
          td= exercise.repMax
          td= exercise.rest
          td
            a(href= exercise.demo) #{click}
         
  script(src='https://code.jquery.com/jquery-3.5.1.min.js' integrity='sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=' crossorigin='anonymous')
  script(src='https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.min.js' integrity='sha384-w1Q4orYjBQndcko6MimVbzY0tgp4pWB4lZ7lr30WKz0vr/aWKhXdBNmNb5D92v7s' crossorigin='anonymous')
  script(src='/javascripts/bootstrap.min.js')       

